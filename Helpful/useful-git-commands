#to set up username info
git init
2.git config user.name "someone"
3.git config user.email "someone@someplace.com"
4.git add *
5.git commit -m "some init msg"

#to add all changes to origin master
git pull
git add . -A
git commit -m "Deleted some files..."
git push origin master

#to check status 
git status

#get back deleted files on local
git checkout HEAD <path> #if not committed
git checkout HEAD^ <path> #if committed


